<UserControl x:Class="三相智慧能源网关调试软件.MyControl.XMLLogControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:三相智慧能源网关调试软件.MyControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" DataContext="{Binding Source={StaticResource Locator},Path=XmlLog}">
    <Grid>
        <TextBox x:Name="TextBoxXML" Text="{Binding XmlLog}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                 TextWrapping="Wrap" 
                 ></TextBox>
    </Grid>
</UserControl>
