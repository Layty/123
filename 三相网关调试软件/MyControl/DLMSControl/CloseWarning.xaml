<UserControl
    x:Class="三相智慧能源网关调试软件.MyControl.DLMSControl.CloseWarning"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:三相智慧能源网关调试软件.MyControl.DLMSControl"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <StackPanel>
        <Button Command="{Binding InitCommand}" Content="Init" />
        <Button Command="{Binding GetWarningConfigCommand}" Content="获取配置字" />
        <Button Content="关闭告警" />
        <Button Command="{Binding ReleaseCommand}" Content="Release" />

    </StackPanel>
</UserControl>
