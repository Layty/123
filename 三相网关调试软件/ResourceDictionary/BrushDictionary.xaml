<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:三相智慧能源网关调试软件.ResourceDictionary">
    <LinearGradientBrush x:Key="RainBallLinearGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <!--红 橙 黄 绿 青 蓝 紫-->
        <GradientStop Color="#FFF0040F" Offset="0"/>
        <GradientStop Color="White" Offset="1"/>
        <GradientStop Color="#FFFB6701" Offset="0.149"/>
        <GradientStop Color="#FFF9EE00" Offset="0.293"/>
        <GradientStop Color="#FF72F902" Offset="0.44"/>
        <GradientStop Color="#FE03FBD9" Offset="0.587"/>
        <GradientStop Color="#FE037FFB" Offset="0.716"/>
        <GradientStop Color="#FE7203F7" Offset="0.846"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LinearGradientBrush" StartPoint="0 0" EndPoint="1 1" >
        <GradientStop Offset="0" Color="Brown" />
        <GradientStop Offset="0.3" Color='Red' />
        <GradientStop Offset="0.6" Color="Orange" />
        <GradientStop Offset="1" Color="Yellow" />
    </LinearGradientBrush>

</ResourceDictionary>